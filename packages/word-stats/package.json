{
  "name": "@kitsuyui/word-stats",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Yui Kitsu <kitsuyui@kitsuyui.com>",
  "description": "Word statistics package (count, TF-IDF, etc.)",
  "scripts": {
    "build": "tsdown --clean",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "validate": "validate-package-exports --check --verify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kitsuyui/ts-playground.git"
  },
  "exports": {
    ".": {
      "require": {
        "type": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "type": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {}
}
