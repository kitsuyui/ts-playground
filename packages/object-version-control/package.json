{
  "name": "@kitsuyui/object-version-control",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Yui Kitsu <kitsuyui@kitsuyui.com>",
  "description": "example",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --no-clean --watch",
    "test": "vitest run",
    "validate": "validate-package-exports --check --verify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kitsuyui/ts-playground.git"
  },
  "exports": {
    ".": {
      "require": {
        "type": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "type": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "dependencies": {
    "js-sha256": "^0.11.1",
    "jsondiffpatch": "^0.7.3"
  },
  "devDependencies": {
    "@automerge/automerge": "^3.2.0",
    "@types/node": "^24.9.2",
    "jsondiffpatch": "^0.7.0"
  },
  "peerDependencies": {
    "@automerge/automerge": "^2.0.0 || ^3.0.0",
    "jsondiffpatch": "^0.6.0 || ^0.7.0"
  }
}
